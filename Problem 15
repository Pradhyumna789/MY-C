#include <stdio.h>

int main()
{
     
     int i = 1, num, sum = 0;
     
     printf("Enter a positive integer: ");
     scanf("%d", &num);
     
   while(i<=num) {
       
       sum = sum + i;
       i++;
       
   }
   
             printf("The sum of %d is: %d", num, sum); 
              
 return 0;
 
}
