#include <stdio.h>

int main() {

int a = 5;
float b = 5.5;
char c = 'A';
int d = 45;
int e = 45 + 4;

printf("The value of a is %d \n", a);
printf("The value of b is %f \n", b);
printf("The value of c is %c \n", c);
printf("The sum of a & d is %d \n", a + d);
printf("The sum of a & d is %d \n", e);

return 0;

}
