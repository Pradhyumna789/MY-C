#include <stdio.h>

int main()
{
     
     int i=1, num, sum = 0;
     
     printf("Enter a positive integer: ");
     scanf("%d", &num);
     
     
    do {
        
        sum = sum + i;
        i++;
        
    } while(i<=num);
    
    printf("The sum of %d is: %d ", num, sum);
              
 return 0;
 
}
