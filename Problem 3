#include <stdio.h>

 int main() 

{
    
    float c; // c is temperature in celcius
    float f; // f is temperature in fahrenheit
    
    printf("The value of celcius in temperature given is???\n");
    scanf("%f", &c);
    
    
    printf("The value of celcius converted to fahrenheit is %f", (c*1.8) + 32);
    
    return 0;
    
     }
